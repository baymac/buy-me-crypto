{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Next: Chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Next: Node",
        "skipFiles": ["<node_internals>/**"],
        "port": 9229
      }
    ],
    "compounds": [
      {
        "name": "Next: Full",
        "configurations": ["Next: Node", "Next: Chrome"]
      }
    ],
    "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
  }